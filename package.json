{
  "name": "@apicase/services",
  "version": "0.8.0",
  "description": "Powerful API layer based on `@apicase/core`",
  "main": "index.cjs.js",
  "module": "index.es.js",
  "repository": "https://github.com/apicase/services",
  "author": "Kelin2025",
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm run check"
    }
  },
  "scripts": {
    "lint": "eslint --fix index.js",
    "test": "jest",
    "check": "npm run lint && npm run test",
    "build": "SET BABEL_ENV=cjs && babel index.js --out-file index.cjs.js && SET BABEL_ENV=es && babel index.js --out-file index.es.js",
    "prepublish": "npm run build && npm run check"
  },
  "dependencies": {
    "@apicase/core": "^0.14.5",
    "delightful-bus": "^0.7.3",
    "nanoutils": "^0.0.17"
  },
  "devDependencies": {
    "@apicase/adapter-fetch": "^0.3.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-nanoutils": "^0.1.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "ghooks": "^2.0.2",
    "jest": "^22.1.4"
  }
}
